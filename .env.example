# Langfuse Configuration
# Copy this file to .env and fill in your values

# Langfuse API Keys (get from https://cloud.langfuse.com)
LANGFUSE_SECRET_KEY=sk-lf-...
LANGFUSE_PUBLIC_KEY=pk-lf-...

# Langfuse Host (choose your region)
# EU: https://cloud.langfuse.com
# US: https://us.cloud.langfuse.com
# HIPAA: https://hipaa.cloud.langfuse.com
# Self-hosted: http://localhost:3000
LANGFUSE_HOST=https://cloud.langfuse.com

# LLM Provider API Keys
OPENAI_API_KEY=sk-...
# ANTHROPIC_API_KEY=sk-ant-...
# GOOGLE_API_KEY=...
# AZURE_OPENAI_API_KEY=...

# Optional: Environment tag for traces
LANGFUSE_ENVIRONMENT=development
# Options: development, staging, production

# Optional: Release/version tracking
LANGFUSE_RELEASE=v1.0.0
